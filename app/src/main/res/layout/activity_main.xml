<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.example.greedaolsbj.MainActivity">

    <com.example.greedaolsbj.MyTitleview
        android:id="@+id/main_mytitle"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"></com.example.greedaolsbj.MyTitleview>

    <com.fynn.fluidlayout.FluidLayout
        android:id="@+id/fluidLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"></com.fynn.fluidlayout.FluidLayout>

    <Button
        android:id="@+id/btn_all"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:text="清除历史记录" />
</LinearLayout>
